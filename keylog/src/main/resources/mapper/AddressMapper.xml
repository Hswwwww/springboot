<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.test.keylog.mapper.AddressMapper">

    <select id="time" resultType="String">
        select sysdate from dual
    </select>
    <select id="view" resultType="com.test.keylog.dto.BoardDTO">
        SELECT * FROM tblBoard
    </select>

    <select id="mypage" parameterType="String" resultType="com.test.keylog.dto.UserDTO">
        select * from tblUser where id= #{id}
    </select>

</mapper>


